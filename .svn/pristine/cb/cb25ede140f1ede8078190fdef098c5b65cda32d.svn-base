<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
    xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
  android:orientation="vertical"  
  android:layout_width="300dp"  
  android:background="@drawable/dialog_background"
  android:layout_height="wrap_content" >
       <com.beardedhen.androidbootstrap.BootstrapEditText
        android:id="@+id/weixin_gailan_autoresoure_item_query_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:hint="id"
        android:singleLine="true"
        android:visibility="gone"
        bootstrapbutton:be_roundedCorners="true"
        bootstrapbutton:be_state="warning" />
      
<TableLayout
             
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center" >
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:gravity="center" >

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_image"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="图片"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_voice"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="语音"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_video"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="视频"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />
                </TableRow>

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:gravity="center" >

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_text"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="文本"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_music"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="音乐"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />

                    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/weixin_gailan_autoresoure_item_query_imagetext"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="图文"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />
                </TableRow>
                </TableLayout>
          
      <ListView 
           android:id="@+id/weixin_gailan_autoresoure_item_query_list"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:scrollbars="none"
          android:divider="#00000000"
          android:gravity="center"
          ></ListView>
</LinearLayout>
