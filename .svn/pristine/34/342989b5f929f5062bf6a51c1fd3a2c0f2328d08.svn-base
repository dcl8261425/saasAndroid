<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
    xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
    android:id="@+id/weixin_wifi_layout_main"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent"
  	android:background="@drawable/dialog_background"
    android:orientation="vertical" >
   <com.beardedhen.androidbootstrap.BootstrapEditText
        android:id="@+id/weixin_wifi_look_nowpage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:hint="id"
        android:singleLine="true"
        android:visibility="gone"
        bootstrapbutton:be_roundedCorners="true"
        bootstrapbutton:be_state="warning" />
    <com.beardedhen.androidbootstrap.BootstrapButton
            android:id="@+id/weixin_wifi_look_layout_item_back"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="取消"
            bootstrapbutton:bb_roundedCorners="true"
            bootstrapbutton:bb_text_gravity="center"
	          android:layout_margin="5dp"
            bootstrapbutton:bb_type="primary" />
       <com.herotculb.qunhaichat.view.listview.updown.XListView
 					android:id="@+id/weixin_wifi_look_list"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="5dp"
                    android:divider="#00000000"
                    android:gravity="center"
                    android:scrollbars="none">
    </com.herotculb.qunhaichat.view.listview.updown.XListView>

</LinearLayout>
