<?xml version="1.0" encoding="utf-8"?>
<TableLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
    xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:shrinkColumns="0"
    android:stretchColumns="0"
     >
     <TableRow 
               android:layout_width="fill_parent"
        android:layout_margin="0dp"
       android:gravity="center" 
        android:paddingBottom="16.0dip"
           android:paddingTop="8.0dip"
            android:layout_marginLeft="2.0dip"
           android:layout_marginRight="2.0dip"
        >
        <TextView android:id="@+id/game_dazhuanpan_content"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:textSize="20dp"
	        android:textStyle="bold"
	                android:dividerHeight="0dp"
	        android:textColor="@color/darksalmon"
	        android:gravity="center"
	        />
        <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/game_dazhuanpan_content_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="开始"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="danger" />
    </TableRow>
    <TableRow 
               android:layout_width="fill_parent"
        android:layout_margin="0dp"
       android:gravity="center" 
        android:paddingBottom="16.0dip"
           android:paddingTop="8.0dip"
            android:layout_marginLeft="2.0dip"
           android:layout_marginRight="2.0dip"
        > 
        <TextView android:id="@+id/game_dazhuanpan_start_date"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:textStyle="bold"
	                android:dividerHeight="0dp"
	        android:textColor="@color/darksalmon"
	        android:gravity="center"/>
        <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/game_dazhuanpan_start_date_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="开始"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="danger" />
     </TableRow>
     <TableRow 
               android:layout_width="fill_parent"
        android:layout_margin="0dp"
       android:gravity="center" 
        android:paddingBottom="16.0dip"
           android:paddingTop="8.0dip"
            android:layout_marginLeft="2.0dip"
           android:layout_marginRight="2.0dip"
        >
        <TextView android:id="@+id/game_dazhuanpan_end_date"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:textStyle="bold"
	                android:dividerHeight="0dp"
	        android:textColor="@color/darksalmon"
	        android:gravity="center"/>
        <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/game_dazhuanpan_end_date_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="结束"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="danger" />
     </TableRow>
     <TableRow 
               android:layout_width="fill_parent"
        android:layout_margin="0dp"
       android:gravity="center" 
        android:paddingBottom="16.0dip"
           android:paddingTop="8.0dip"
            android:layout_marginLeft="2.0dip"
           android:layout_marginRight="2.0dip"
        >
        <TextView android:id="@+id/game_dazhuanpan_num"
	        android:layout_width="match_parent"
	        android:layout_height="50dp"
	        android:textSize="20dp"
	        android:textStyle="bold"
	                android:dividerHeight="0dp"
	        android:textColor="@color/darksalmon"
	        android:gravity="center"
	        />
     </TableRow>
     <TableRow 
               android:layout_width="fill_parent"
        android:layout_margin="0dp"
       android:gravity="center" 
        android:paddingBottom="16.0dip"
           android:paddingTop="8.0dip"
            android:layout_marginLeft="2.0dip"
           android:layout_marginRight="2.0dip"
        >      
	    <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/game_dazhuanpan_enter"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_margin="2dp"
                        android:text="确认设置"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="danger" /> 
    </TableRow>

</TableLayout>
