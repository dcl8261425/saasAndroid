	@Override
	public List<Map<String, Object>> getAutoReSend_Text() {
		// TODO Auto-generated method stub
		List<NameValuePair> params=new ArrayList<NameValuePair>();
		List<Map<String,Object>> list=new ArrayList<Map<String,Object>>();
		Map map=new HashMap<String, Object>();
		try {
			String json=HttpClient.doPost(QNPermissionApi.mainLink+QNPermissionApi.getText, params);
			 JSONTokener jsonParser = new JSONTokener(json);  
			 Log.e("json", json);
			 JSONObject person = (JSONObject) jsonParser.nextValue();
			 JSONObject jsonSuccess=person.getJSONObject("success");
			 
			 String info=person.getString("info");
			
			 if(!jsonSuccess.getBoolean("success")){
				 Object islogin=null;
					 try{
						 islogin=person.get("stute");
					 }catch(Exception ex){
						 map.put("login", false);
						 map.put("success", false);
						 list.add(map);
						 return list;
					 }
					 map.put("success", false);
					 map.put("info", "获取数据失败");
					 list.add(map);
					 return list;
			 }else{
				 //解析data
				 List<AotuSendImageTextMessageDto> imageList=new ArrayList<AotuSendImageTextMessageDto>();
				 JSONArray jsonIMlist=person.getJSONArray("data");
				 for (int i = 0; i < jsonIMlist.length(); i++) {
					 AotuSendImageTextMessageDto dto = new AotuSendImageTextMessageDto();
					 JSONObject jbt=jsonIMlist.getJSONObject(i);
					 dto.setCreateTime(jbt.getString("createTime"));
					 dto.setFromUserName(jbt.getString("fromUserName"));
					 dto.setMsgType(jbt.getString("msgType"));
					 dto.setToUserName(jbt.getString("toUserName"));
					 dto.setArticleCount(jbt.getString("articleCount"));
					 dto.setArticles(new AotuSendImageTextMessageContentListDto());
					 imageList.add(dto);
				}
				 map.put("data", imageList);
			 }
			 map.put("success", jsonSuccess);
			 list.add(map);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			 map.put("success", false);
			 map.put("info", "登录异常");
			 list.add(map);
		}
		return list;
	}
