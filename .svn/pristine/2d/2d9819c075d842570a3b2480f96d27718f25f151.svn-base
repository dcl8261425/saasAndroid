<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
    xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/query_goods_item_man"
    android:gravity="center"
    android:orientation="vertical" >
     <TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_id"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:visibility="gone"
                  />
    		<TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_name"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  />
          <TableRow 
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:gravity="center"
              >
				
               <TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_stute"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  />
                 <TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_createDate"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  />
               <TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_createMan"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  />
                <TextView 
                   android:layout_weight="1"
                  android:id="@+id/inorder_rukuMan"
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  />
                
          </TableRow>
 <TableRow 
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:gravity="center"
              >
                <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/query_goods_item_type_enter"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="查看"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />
                <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/query_goods_item_type_enter"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="入库"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />
                <com.beardedhen.androidbootstrap.BootstrapButton
                        android:id="@+id/query_goods_item_type_enter"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_margin="5dp"
                        android:text="删除"
                        bootstrapbutton:bb_icon_left="fa-th-list"
                        bootstrapbutton:bb_roundedCorners="true"
                        bootstrapbutton:bb_text_gravity="center"
                        bootstrapbutton:bb_type="primary" />
                </TableRow>
             
             
               
               
</TableLayout>
