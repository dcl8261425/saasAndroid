<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
   	 xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
     <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
    <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_name"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	         android:layout_weight="1"
	        android:hint="输入名字"
	        
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
     <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_marks"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	         android:layout_weight="1"
	        android:hint="输入备注"
	        
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
     </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
        <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="单价"
                   android:layout_weight="1"
                  />
        <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_price"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:layout_weight="1"
	        android:hint="输入价格"
	        android:inputType="numberDecimal"
            android:numeric="integer" 
	        
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
        <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="积分"
                   android:layout_weight="1"
                  />
         <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_score"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:layout_weight="1"
	        android:hint="输入积分分数"
	        android:inputType="numberDecimal"
            android:numeric="integer" 
	        
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
         <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="数量"
                   android:layout_weight="1"
                  />
     <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_num"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:layout_weight="1"
	        android:hint="输入数量"
	        android:inputType="numberDecimal"
            android:numeric="integer" 
	        
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
         <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="型号"
                   android:layout_weight="1"
                  />
          <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_model"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="型号"
			android:layout_weight="1"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
           </TableRow>
      <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
          <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="单价"
                   android:layout_weight="1"
                  />
      <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_type"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="单位"
	        
	        android:layout_weight="1"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
      <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="进价"
                   android:layout_weight="1"
                  />
      <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_inprice"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="进价"
	         android:inputType="numberDecimal"
            android:numeric="integer" 
	        android:layout_weight="1"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
     
      </TableRow>
      <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
      <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="首拼"
                   android:layout_weight="1"
                  />
       <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_pinyin"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="首拼"

			android:layout_weight="1"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
      <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="总价"
                   android:layout_weight="1"
                  />
       <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_countPrice"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:layout_weight="1"
	        android:hint="总价"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
      </TableRow>
       <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
       <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="厂家"
                   android:layout_weight="1"
                  />
       <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_changjia"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="厂家"
			android:layout_weight="1"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
       <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_ingoods_item_changjia_id"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="厂家"
	        android:visibility="gone"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
      <TextView 
                 
                  android:layout_width="fill_parent"
                  android:gravity="center"
                  android:layout_margin="5dp"
                  android:text="仓库"
                   android:layout_weight="1"
                  />
       <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_outgoods_item_cangku"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:layout_weight="1"
	        android:hint="仓库"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
              <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_ingoods_item_cangku_id"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="5dp"
	        android:hint="仓库"
	        	        android:visibility="gone"
	        bootstrapbutton:be_roundedCorners="true"
	        bootstrapbutton:be_state="warning" />
      </TableRow>
       <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
       <com.beardedhen.androidbootstrap.BootstrapButton
                android:id="@+id/good_outgoods_item_enter"
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:text="确定"
                
                bootstrapbutton:bb_icon_left="fa-pencil-square-o"
                bootstrapbutton:bb_roundedCorners="true"
                bootstrapbutton:bb_text_gravity="center"
                bootstrapbutton:bb_type="success" />
        <com.beardedhen.androidbootstrap.BootstrapButton
                android:id="@+id/good_outgoods_item_cel"
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:text="取消"
                
                bootstrapbutton:bb_icon_left="fa-pencil-square-o"
                bootstrapbutton:bb_roundedCorners="true"
                bootstrapbutton:bb_text_gravity="center"
                bootstrapbutton:bb_type="success" />
       </TableRow>
</TableLayout>