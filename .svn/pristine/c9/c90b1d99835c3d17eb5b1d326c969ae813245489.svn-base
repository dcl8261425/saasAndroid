<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
      xmlns:bootstrapbutton="http://schemas.android.com/apk/res-auto"
    xmlns:fontawesometext="http://schemas.android.com/apk/res-auto"
    android:layout_width="300dp"
    android:layout_height="match_parent"
      android:background="@drawable/dialog_background"
    android:orientation="vertical" >
        <TextView 
        android:id="@+id/good_update_goods_price_good_id"
        android:layout_width="fill_parent"
          android:layout_margin="10dp"
          android:visibility="gone"
        android:layout_height="wrap_content"
        />
    <TextView 
        android:id="@+id/good_update_goods_price_oldPrice"
        android:layout_width="fill_parent"
          android:layout_margin="10dp"
        android:layout_height="wrap_content"
        />
	 <com.beardedhen.androidbootstrap.BootstrapEditText
	        android:id="@+id/good_update_goods_price_newPrice"
	        android:layout_width="match_parent"
	        android:inputType="numberDecimal"
	        android:layout_height="wrap_content"
	        android:layout_margin="10dp"
	        android:hint="要修改的价格"
	        android:singleLine="true"
	        bootstrapbutton:be_roundedCorners="true"
	        android:numeric="integer" 
	        bootstrapbutton:be_state="warning" />
	 <com.beardedhen.androidbootstrap.BootstrapButton
                android:id="@+id/good_update_goods_price_enter"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:text="确定"
                bootstrapbutton:bb_roundedCorners="true"
                bootstrapbutton:bb_text_gravity="center"
                bootstrapbutton:bb_type="danger" />
	 <com.beardedhen.androidbootstrap.BootstrapButton
                android:id="@+id/good_update_goods_price_cenl"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:text="取消"	
                bootstrapbutton:bb_roundedCorners="true"
                bootstrapbutton:bb_text_gravity="center"
                bootstrapbutton:bb_type="danger" />
</LinearLayout>
